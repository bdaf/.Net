@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    @foreach (var item in Model.People) {
        <p>@Html.DisplayFor(ModelItem => item.Id).
        @Html.DisplayFor(ModelItem => item.FirstName)
        @Html.DisplayFor(ModelItem => item.LastName)</p>
    }
</div>
